git push -u origin main


cd "/c/Users/lguimaraes/Downloads/Nova pasta/moneyhub"

git status                    # opcional, só para ver o que mudou
git add .                     # adiciona tudo (novo/alterado)
git commit -m "chore: atualiza X e Y"   # descreva a mudança
git push                      # graças ao -u, não precisa repetir origin main
